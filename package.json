{
  "name": "npm-packages",
  "version": "0.0.0",
  "private": true,
  "description": "displays stats about my npm packages",
  "main": "index.js",
  "scripts": {
    "browserify": "browserify src/index.js -o docs/bundle.js -t babelify",
    "build": "npm run browserify",
    "flow": "flow",
    "flow-typed": "npm run flow-typed_react-redux",
    "flow-typed_react-redux": "flow-typed install react-redux@5.x.x",
    "lint": "standard --fix",
    "postversion": "git push origin v${npm_package_version}; git push origin master",
    "postinstall": "npm run flow-typed",
    "start": "budo src/index.js --open -- -t babelify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/npm-packages.git"
  },
  "keywords": [
    "npm",
    "stats",
    "react"
  ],
  "homepage": "http://g14n.info/npm-packages",
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "standard": {
    "global": [
      "Package"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/npm-packages/issues"
  },
  "devDependencies": {
    "dot-editorconfig": "^1.1.0"
  },
  "dependencies": {
    "babel-eslint": "^8.2.3",
    "eslint-plugin-flowtype": "^2.46.2",
    "flow-bin": "^0.69.0",
    "flow-typed": "^2.4.0",
    "redux-thunk": "^2.2.0",
    "standard": "^11.0.1",
    "zeroconf-redux": "^3.0.0"
  }
}
